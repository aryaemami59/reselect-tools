{
  "name": "reselect-tools",
  "version": "0.0.7",
  "description": "Selector Debugging Tools for Reselect.",
  "jsnext:main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module-sync": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "module": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/skortchmark9/reselect-tools"
  },
  "scripts": {
    "lint": "eslint --flag unstable_ts_config -c eslint.config.mts",
    "clean": "rimraf dist",
    "lint-fix": "npm run lint -- --fix",
    "prepack": "npm run build",
    "test": "vitest --run",
    "example": "opn http://localhost:9080/examples/demo.html && static-server",
    "type-check": "tsc -p tsconfig.json",
    "build": "rimraf dist && tsup --config tsup.config.mts",
    "format": "prettier -w .",
    "format-check": "prettier -c ."
  },
  "keywords": [
    "redux",
    "reselect"
  ],
  "authors": [
    "Sam Kortchmar"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skortchmark9/reselect-tools"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@microsoft/api-extractor": "^7.47.11",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.9.0",
    "@vitest/eslint-plugin": "^1.1.7",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "jiti": "^2.4.0",
    "prettier": "^3.3.3",
    "reselect": "^5.1.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "reselect": "^5.1.0"
  },
  "peerDependenciesMeta": {
    "reselect": {
      "optional": true
    }
  }
}
